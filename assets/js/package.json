{
  "name": "marker-animation",
  "version": "2.0.4",
  "description": "marker animation for wordpress",
  "main": "marker-animation.min.js",
  "scripts": {
    "test": "npm run lint && npm run cover",
    "lint": "eslint src/**/**/*.js && eslint test/**/**/*.js",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --recursive",
    "develop": "npm run develop:m && npm run develop:g",
    "develop:m": "webpack --mode development --config marker-animation.config.js",
    "develop:g": "webpack --mode development --config gutenberg.config.js",
    "build": "npm run build:m && npm run build:g",
    "build:m": "webpack --mode production --config marker-animation.config.js",
    "build:g": "webpack --mode production --config gutenberg.config.js"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@technote-space/register-grouped-format-type": "^1.0.17",
    "jquery.marker-animation": "^1.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "sass-loader": "^7.1.0",
    "should": "^13.2.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-cli": "~3.3.5"
  }
}
