{
  "name": "marker-animation",
  "version": "4.0.5",
  "description": "marker animation for wordpress",
  "main": "marker-animation.min.js",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "yarn develop:m && yarn develop:g",
    "develop:m": "webpack --mode development --config marker-animation.config.js",
    "develop:g": "webpack --mode development --config gutenberg.config.js",
    "build": "yarn build:m && yarn build:g",
    "build:m": "webpack --mode production --config marker-animation.config.js",
    "build:g": "webpack --mode production --config gutenberg.config.js",
    "update": "ncu -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@technote-space/gutenberg-test-helper": "^0.0.18",
    "@technote-space/gutenberg-utils": "^2.2.8",
    "@technote-space/register-grouped-format-type": "^2.0.8",
    "@wordpress/components": "^10.0.3",
    "@wordpress/compose": "^3.19.3",
    "@wordpress/element": "^2.16.0",
    "@wordpress/rich-text": "^3.20.3",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^26.2.2",
    "jest-extended": "^0.11.5",
    "jquery": "^3.5.1",
    "jquery.marker-animation": "^1.4.26",
    "react": "^16.13.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "webpack": "^4.44.1",
    "webpack-cli": "~3.3.12"
  }
}
